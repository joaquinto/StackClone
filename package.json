  "scripts": {
    "build": "rimraf ./dist/ && babel src --out-dir dist",
    "start": "node ./dist/index.js",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "refresh": "babel-node ./src/database/migration/refreshDatabase refreshDatabase",
    "test": "NODE_ENV=test nyc mocha --timeout 1000 -r @babel/register src/test/*.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "heroku-postbuild": "npm run build && npm run refresh && npm run start"
  },
